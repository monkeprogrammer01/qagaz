generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Application {
  id          Int      @id @default(autoincrement())
  clientName  String
  phone       String
  room     String
  status      String   @default("new")
  createdAt   DateTime @default(now())
}